{
  "expo": {
    "name": "anafin",
    "slug": "anafin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AnaFin-icon.png",
    "scheme": "anafin",
    "userInterfaceStyle": "automatic",

    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anafin.analisisfinanciero.app",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AnaFin-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.amca.anafinapp.v2",
      "versionCode": 1
    },

    "plugins": [
      "expo-router",

      [
        "@react-native-firebase/app",
        {
          "ios": {
            "use_frameworks": "static"
          }
        }
      ],

      "@react-native-google-signin/google-signin",

      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "65fdab4f-1ae3-44ad-b7c4-7a707365c0bc"
      }
    },

    "owner": "pablo.1194"
  }
}